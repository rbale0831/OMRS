    <title>Regster page </title>
</head>
<body>
    <header id="head">Registration page </header>
     <div class="container">
         <div class="Enroll">Registration Form</div>
         <form action="">
             <div class="user-details">
                 <div class="inputbox"> 
                     <span class="details">First Name </span>
                     <input type="text"  required>
                 </div>
                 <div class="inputbox"> 
                     <span class="details">Last Name </span>
                     <input type="text"  required>
                 </div>
                 <div class="inputbox"> 
                     <span class="details">Email</span>
                     <input type="email"  required>
                 </div>
                 <!-- <div class="inputbox"> 
                     <span class="details">Phone Number</span>
                     <input type="string" placeholder="Enter Your Number" required>
                 </div> -->
                 <div class="inputbox">  
                     <span class="details">Password</span>
                     <input type="password"  required>
                 </div>
                 <div class="inputbox">  
                     <span class="details">Confirm Password</span>
                     <input type="password"  required>
                 </div>
                 <div class="inputbox">  
                    <span class="details">Blood Pressure </span>
                    <input type="password"  required>
                </div>
                
                 <div class="inputbox">  
                     <span class="details">Address Line 1</span>
                     <input type="address"  required>
                 </div>
                 <div class="inputbox">  
                    <span class="details">Address Line 2</span>
                    <input type="address"  required>
                </div><div class="inputbox">  
                    <span class="details">Locality</span>
                    <input type="address"  required>
                </div><div class="inputbox">  
                    <span class="details">District</span>
                    <input type="address"  required>
                </div><div class="inputbox">  
                    <span class="details">State</span>
                    <input type="address"  required>
                </div>
                <div class="inputbox">  
                    <span class="details">Pincode</span>
                    <input type="address"  required>
                </div>
                <div class="inputbox">  
                    <span class="details">Occupation</span>
                    <input type="address"  required>
                </div><div class="inputbox">  
                    <span class="details">Primary Mobile No.</span>
                    <input type="string"  required>
                </div>
                <div class="inputbox">  
                    <span class="details">Secondary Mobile No.</span>
                    <input   required>
                </div>
                <div class="inputbox">  
                    <span class="details">Date Of Birth</span>
                    <input  required>
                </div>
             </div>
             
             <div class="gender-details">
                 <span class="gender-title">Gender</span>
                 <div class="category">
                     <label for="">
                         <span class="gender">Male</span>
                         <input type="radio"name="gender">
                     </label>
                     <label for="">
                         <span class="gender">Female</span>
                         <input type="radio" name='gender'>
                    </label>
                     <label for="">
                         <span class="gender">Other</span>
                         <input type="radio" name='gender'>
                    </label>
                </div>
             </div>
             <div class="button">
                <input type="submit" value="Create">
             </div>
         </form>
</body>
</html>